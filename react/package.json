{
  "name": "@haiilo/catalyst-react",
  "version": "14.1.0",
  "description": "React wrapper for Catalyst Design System",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "prebuild": "pnpm run clean",
    "build": "pnpm run tsc",
    "tsc": "tsc -p .",
    "clean": "rm -rf ./dist",
    "reset": "rm -rf ./dist ./node_modules"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@haiilo/catalyst": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node": "^15.12.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "jest": "29.7.0",
    "@testing-library/jest-dom": "6.9.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "typescript": "5.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
