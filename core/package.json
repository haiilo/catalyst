{
  "name": "@haiilo/catalyst",
  "version": "14.1.0",
  "description": "Catalyst Design System",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haiilo/catalyst.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/catalyst/catalyst.esm.js",
  "customElements": "dist/custom-elements/custom-elements.json",
  "files": [
    "dist/",
    "loader/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "stencil build --docs",
    "build:watch": "stencil build --docs --watch",
    "build:ci": "stencil build --ci",
    "postbuild": "node replace",
    "postbuild:ci": "node replace",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "generate": "stencil generate",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:style": "stylelint **/*.scss",
    "clean": "rm -rf ./.stencil ./dist ./loader ./www",
    "reset": "rm -rf ./.stencil ./dist ./loader ./www ./node_modules"
  },
  "peerDependencies": {
    "@haiilo/catalyst-icons": ">=1.0.0",
    "rxjs": "^7.5.5"
  },
  "dependencies": {
    "@floating-ui/dom": "1.6.13",
    "@haiilo/catalyst-tokens": "workspace:*",
    "@stencil/core": "4.38.3",
    "autosize": "6.0.1",
    "autosize-input": "1.0.2",
    "cleave-zen": "0.0.17",
    "flatpickr": "4.6.13",
    "focus-trap": "7.6.4",
    "loglevel": "1.9.2",
    "sanitize.css": "13.0.0",
    "tabbable": "6.2.0",
    "toastify-js": "1.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@haiilo/catalyst-icons": "^2.6.1",
    "@stencil/angular-output-target": "1.1.1",
    "@stencil/react-output-target": "0.5.3",
    "@stencil/sass": "3.2.3",
    "@types/autosize": "^4.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.2",
    "@types/toastify-js": "^1.12.3",
    "@typescript-eslint/eslint-plugin": "8.52.0",
    "@typescript-eslint/parser": "8.52.0",
    "babel-loader": "9.2.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "9.1.2",
    "jest": "30.2.0",
    "jest-cli": "30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-stencil-runner": "0.0.18",
    "prettier": "3.7.4",
    "puppeteer": "24.32.0",
    "replace-in-file": "8.4.0",
    "rxjs": "7.8.2",
    "stencil-inline-svg": "1.1.0",
    "stylelint": "16.26.1",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "typescript": "5.9.3"
  }
}
