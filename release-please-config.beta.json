{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    "tokens": {
      "component": "catalyst-tokens",
      "release-type": "node",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": true,
      "prerelease-type": "beta",
      "versioning": "prerelease"
    },
    "core": {
      "component": "catalyst-core",
      "release-type": "node",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": true,
      "prerelease-type": "beta",
      "versioning": "prerelease"
    },
    "angular/projects/catalyst": {
      "component": "catalyst-angular",
      "release-type": "node",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": true,
      "prerelease-type": "beta",
      "versioning": "prerelease"
    },
    "angular/projects/catalyst-formly": {
      "component": "catalyst-angular-formly",
      "release-type": "node",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": true,
      "prerelease-type": "beta",
      "versioning": "prerelease"
    },
    "react": {
      "component": "catalyst-react",
      "release-type": "node",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": true,
      "prerelease-type": "beta",
      "versioning": "prerelease"
    }
  },
  "plugins": [
    {
      "type": "linked-versions",
      "groupName": "catalyst",
      "components": [
        "catalyst-tokens",
        "catalyst-core",
        "catalyst-angular",
        "catalyst-angular-formly",
        "catalyst-react"
      ]
    }
  ]
}
